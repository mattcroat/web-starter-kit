{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["airbnb-base", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "semi": ["error", "never"],
    "quotes": ["error", "backtick"],
    "no-console": "off",
    "indent": ["error", "tab"]
  }
}
